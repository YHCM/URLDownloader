<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="525.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="downloader.order.OrderController">
    <Label layoutX="32.0" layoutY="28.0" text="开始：">
        <font>
            <Font size="21.0"/>
        </font>
    </Label>

    <!--开始数字输入框-->
    <TextField fx:id="startText" layoutX="95.0" layoutY="21.0" prefHeight="40.0" prefWidth="108.0" promptText="输入整数">
        <font>
            <Font size="21.0"/>
        </font>
    </TextField>
    <Label layoutX="319.0" layoutY="28.0" text="结束：">
        <font>
            <Font size="21.0"/>
        </font>
    </Label>

    <!--结束数字输入框-->
    <TextField fx:id="endText" layoutX="382.0" layoutY="21.0" prefHeight="40.0" prefWidth="108.0" promptText="输入整数">
        <font>
            <Font size="21.0"/>
        </font>
    </TextField>

    <!--URL例子输入框-->
    <TextField fx:id="exampleText" layoutX="32.0" layoutY="75.0" prefHeight="40.0" prefWidth="317.0" promptText="输入URL">
        <font>
            <Font size="21.0"/>
        </font>
    </TextField>

    <!--执行按钮-->
    <Button fx:id="outputButton" layoutX="382.0" layoutY="75.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="108.0" text="生成">
        <font>
            <Font size="21.0"/>
        </font>
    </Button>
</AnchorPane>
